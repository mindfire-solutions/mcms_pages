<!--
  calling libray class method get_libraries of class AssetManager
  to load specified js files listed in controller
-->
<% Asset.get_libraries.each do |library| %>

  <!-- include specific javascript file to page -->
  <%= javascript_include_tag  library %>

<% end %>

